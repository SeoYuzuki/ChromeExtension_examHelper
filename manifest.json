{
  "manifest_version": 2,
  "name": "guraExamHelper",
  "description": "guraExamHelper",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "commands": {},
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["*://*/*"],
      "js": [
        "execute.js",
        "floatingDivs.js",
        "examDataBase.js",
        "tools/jquery.min.js"
      ]
    }
  ]
}
